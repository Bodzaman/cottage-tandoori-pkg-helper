{
  "name": "cottage-tandoori-pkg-helper",
  "version": "1.0.3",
  "description": "POS Printer Helper for Cottage Tandoori Restaurant",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "pkg package.json --compress Brotli"
  },
  "pkg": {
    "targets": [
      "node18-win-x64",
      "node18-macos-x64", 
      "node18-linux-x64"
    ],
    "outputPath": "dist",
    "options": [
      "--compress",
      "Brotli"
    ]
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "pos",
    "printer", 
    "restaurant",
    "tandoori"
  ]
}