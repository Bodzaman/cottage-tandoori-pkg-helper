{
  "name": "cottage-tandoori-pkg-helper",
  "version": "1.0.2",
  "description": "Simplified PKG-based printer helper for Cottage Tandoori Restaurant",
  "main": "index.js",
  "bin": "./index.js",
  "scripts": {
    "start": "node index.js",
    "build": "pkg . --targets node18-linux-x64,node18-win-x64,node18-macos-x64 --compress Brotli --out-path dist",
    "test": "node index.js"
  },
  "pkg": {
    "scripts": [
      "index.js"
    ],
    "assets": [],
    "targets": [
      "node18-linux-x64",
      "node18-win-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "pos",
    "printer",
    "restaurant",
    "cottage-tandoori",
    "pkg"
  ],
  "author": "Cottage Tandoori Restaurant",
  "license": "MIT"
}